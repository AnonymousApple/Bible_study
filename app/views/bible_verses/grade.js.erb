$('#score-answer').html('Your score is: <%= j @correct %>')
$('#score-answer').show(1000)
<% 3.times do %>
  <% if @correct >= 0.95 %>
    $('#score-answer').html('Yaaaaaaay!!!')
  <% else %>
    $('#score-answer').html('Uh oh!!!!<a href>Try again.</a>')
  <% end %>
<% end %>
$('form[method="post"]').hide()
